<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.teamadams.staqr.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_web"
        android:name="com.teamadams.staqr.ui.web.WebFragment"
        android:label="@string/title_web"
        tools:layout="@layout/fragment_web" />

    <fragment
        android:id="@+id/navigation_dialer"
        android:name="com.teamadams.staqr.ui.dialer.DialerFragment"
        android:label="@string/title_dialer"
        tools:layout="@layout/fragment_dialer" />

    <fragment
        android:id="@+id/navigation_qrcode"
        android:name="com.teamadams.staqr.ui.qrcode.QRcodeFragment"
        android:label="@string/title_qrcode"
        tools:layout="@layout/fragment_qrcode" />
</navigation>
